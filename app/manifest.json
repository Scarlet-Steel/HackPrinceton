
{
 "name": "Color Blindness Program",
 "version": "1.0",
 "manifest_version": 2,
 "authors": "Edward Cai, Hill Chang, Matthew Chin, Julian Gau"
  "background": {"scripts": ["background.js"],
   "persistent":true},
  "description": "This Google Chrome extension changes color on websites for color blind people.",
  "permissions": ["tabs"],
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"icons": {
		"48": "icon-48.png",
		"128": "icon-128.png"
	},
   "content_scripts": [
   {
     "matches": ["<all_urls>"],
     "css": ["mystyles.css"],
     "js": ["jquery-1.10.1.min.js","contentscript.js"] ,
     "run_at" : "document_end"
   }]
}

